{
  "name": "Angel List",
  "slug": "angel",
  "url": "https://angel.co/",
  "auth_types": {
    "Authorized": {
      "setup_help": "./samples/getting_started_authorized.md",
      "requires_signing": true
    },
    "Unauthorized": {
      "setup_help": "./samples/getting_started_unauthorized.md"
    }
  },
  "settings": [{
    "key": "clientId",
    "type": "text",
    "description": "Client Id",
    "for_types": ["Authorized"]
  }, {
    "key": "clientSecret",
    "type": "text",
    "description": "Client Secret",
    "for_types": ["Authorized"]
  }],
